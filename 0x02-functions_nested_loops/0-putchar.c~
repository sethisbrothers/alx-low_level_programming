#include <stdio.h>
/*
 *main - prints _putchar
 *
 *Return: Always 0 (Success)
 *
 */
int main(void)
{
  int str[] = {95, 112, 117, 116, 99, 104, 97, 114};
  int string, st;
  st = sizeof(str) / sizeof(int);
  for (string = 0; string < st; string++)
    {
      _putchar(str[string]);
    }
  _putchar('\n');

  return (0);
 }
